#!/bin/bash
#SBATCH -J  T0951
#SBATCH -o T0951-%j.out
#SBATCH --partition Lewis,hpc5,hpc4
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=10
#SBATCH --mem-per-cpu=2G
#SBATCH --time 2-00:00

mkdir -p SOFTWARE_PATH/test_out/T0951_multicom/
cd SOFTWARE_PATH/test_out/T0951_multicom/

if [[ ! -f "SOFTWARE_PATH/test_out/T0951_multicom/mcomb/casp1.pdb" ]];then 
	perl SOFTWARE_PATH/bin/run_multicom_Human.sh T0951 SOFTWARE_PATH/examples/T0951.fasta SOFTWARE_PATH/test_out/T0951_multicom/ SOFTWARE_PATH/examples/T0951

fi


printf "\nFinished.."
printf "\nCheck log file <SOFTWARE_PATH/test_out/T0951_multicom.log>\n\n"
